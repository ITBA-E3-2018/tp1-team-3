#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xaaf330 .scope module, "ej5TestBench" "ej5TestBench" 2 3;
 .timescale 0 0;
v0xabf810_0 .net "Digit0", 3 0, v0xabf620_0; 1 drivers
v0xabf8e0_0 .net "Digit1", 3 0, v0xabf6c0_0; 1 drivers
v0xabf990_0 .var "Num1", 3 0;
v0xabfa40_0 .var "Num2", 3 0;
v0xabfb20_0 .var/i "i", 31 0;
v0xabfba0_0 .var/i "j", 31 0;
S_0xaaf420 .scope module, "Sumador" "bcdSum" 2 9, 3 1, S_0xaaf330;
 .timescale 0 0;
v0xa78690_0 .net "A", 3 0, v0xabf990_0; 1 drivers
v0xabf580_0 .net "B", 3 0, v0xabfa40_0; 1 drivers
v0xabf620_0 .var "BCD0", 3 0;
v0xabf6c0_0 .var "BCD1", 3 0;
v0xabf770_0 .var "C", 7 0;
E_0xa89da0 .event edge, v0xabf580_0, v0xa78690_0;
    .scope S_0xaaf420;
T_0 ;
    %wait E_0xa89da0;
    %load/v 8, v0xa78690_0, 4;
    %mov 12, 0, 4;
    %load/v 16, v0xabf580_0, 4;
    %mov 20, 0, 4;
    %add 8, 16, 8;
    %set/v v0xabf770_0, 8, 8;
    %set/v v0xabf6c0_0, 0, 4;
    %movi 8, 9, 9;
    %load/v 17, v0xabf770_0, 8;
    %mov 25, 0, 1;
    %cmp/u 8, 17, 9;
    %jmp/0xz  T_0.0, 5;
    %load/v 8, v0xabf770_0, 8;
    %mov 16, 0, 24;
    %subi 8, 10, 32;
    %set/v v0xabf770_0, 8, 8;
    %movi 8, 1, 4;
    %set/v v0xabf6c0_0, 8, 4;
T_0.0 ;
    %load/v 8, v0xabf770_0, 8;
    %set/v v0xabf620_0, 8, 4;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xaaf330;
T_1 ;
    %vpi_call 2 12 "$display", "Sumador BCD";
    %set/v v0xabfb20_0, 0, 32;
T_1.0 ;
    %load/v 8, v0xabfb20_0, 32;
   %cmpi/s 8, 10, 32;
    %jmp/0xz T_1.1, 5;
    %set/v v0xabfba0_0, 0, 32;
T_1.2 ;
    %load/v 8, v0xabfba0_0, 32;
   %cmpi/s 8, 10, 32;
    %jmp/0xz T_1.3, 5;
    %load/v 8, v0xabfb20_0, 32;
    %set/v v0xabf990_0, 8, 4;
    %load/v 8, v0xabfba0_0, 32;
    %set/v v0xabfa40_0, 8, 4;
    %delay 1, 0;
    %vpi_call 2 18 "$display", "%d + %d =  %d %d", v0xabfb20_0, v0xabfba0_0, v0xabf8e0_0, v0xabf810_0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0xabfba0_0, 32;
    %set/v v0xabfba0_0, 8, 32;
    %jmp T_1.2;
T_1.3 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0xabfb20_0, 32;
    %set/v v0xabfb20_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 29 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "src/ej5TestBench.v";
